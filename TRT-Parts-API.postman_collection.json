							"mode": "raw",
							"raw": "{\n  \"username\": \"superadmin\",\n  \"password\": \"SuperAdmin123!\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseUrl}}/auth/login",
							"host": ["{{baseUrl}}"],
							"path": ["auth", "login"]
